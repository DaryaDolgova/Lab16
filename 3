# Lab16
using namespace std;

#include <iostream>

int main() 
{
	int  N, i, max, min, ma, mi, x;
	cout << "N=";  cin >> N;
	int* A = new int[N];
	int* B = new int[N + 2];
	
	x = 0;
	for (i = 0; i < N; i++) 
	{
		cin >> A[i];
	}
	
	max = A[0]; min = A[0]; ma = 0; mi = 0;
	for (i = 0; i < N; i++) 
	{
		if (A[i] > max) { max = A[i]; ma = i; }
		if (A[i] < min) { min = A[i]; mi = i; }
	}
	for (i = 0; i < N; i++) {
		if (A[i] == min) {
			B[i + x] = 0;
			B[i + x + 1] = A[i];
			min=min-1;
			x++;
		}
		if (A[i] == max) {
			B[i + x] = A[i];
			B[i + x + 1] = 0;
			max++;
			x++;
		}
		else { B[i + x] = A[i]; }
	}
	
	for (i = 0; i < N + 2; i++) 
	{
		cout << B[i] << endl;
	}

	return 0;

}
